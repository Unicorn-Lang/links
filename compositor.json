    },
    {
      "component": "footer/BasicFooter",
      "heading": "Copyright 2016",
      "links": [
        {
          "href": "http://twitter.com/mrmrs_",
          "text": "Twitter"
        },
        {
          "href": "http://github.com/mrmrs",
          "text": "GitHub"
        },
        {
          "href": "http://unsplash.com/mrmrs",
