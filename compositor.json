{
  "name": "mrmrs/links",
  "version": "0.1.0",
  "libraries": {
    "xv": "1.0.0"
  },
  "style": {
    "fontFamily": "-apple-system, BlinkMacSystemFont, sans-serif",
    "fontWeight": 400,
    "bold": 600,
    "lineHeight": 1.5,
    "typeScale": [
      31,
      29,
      23,
      19,
      17,
      13,
      11
